message = "global"

def outer():
    message = "local"

    def inner():

        nonlocal message  # nonlocal variables only use in nested functions
        message = 'nonlocal'
        def inner2():
            nonlocal message

            message = 'nonlocal 2'
            print("inner 2:",message)
        inner2()
        #message = "nonlocal"
        print("inner:", message)

    inner()
    print("outer:", message)

outer()
print("main:", message)





