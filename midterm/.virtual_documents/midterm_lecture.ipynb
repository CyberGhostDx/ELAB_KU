def outer_function():
    global num
    num = 20

    def inner_function():
        global num
        num = 25
        #print("call in inner_..:", num)

    print('Before calling inner_..:', num)
    inner_function()
    print('After calling inner_..:', num)
    
outer_function()
print('Outside both functions: ', num)





